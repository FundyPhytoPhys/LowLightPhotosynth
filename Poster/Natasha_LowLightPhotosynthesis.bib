@article{mirkovicLightAbsorptionEnergy2017a,
  title = {Light {{Absorption}} and {{Energy Transfer}} in the {{Antenna Complexes}} of {{Photosynthetic Organisms}}},
  author = {Mirkovic, Tihana and Ostroumov, Evgeny E. and Anna, Jessica M. and {van Grondelle}, Rienk and {Govindjee} and Scholes, Gregory D.},
