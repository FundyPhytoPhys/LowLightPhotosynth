@article{schubackSingleTurnoverVariableChlorophyll2021,
  title = {Single-{{Turnover Variable Chlorophyll Fluorescence}} as a {{Tool}} for {{Assessing Phytoplankton Photosynthesis}} and {{Primary Productivity}}: {{Opportunities}}, {{Caveats}} and {{Recommendations}}},
  shorttitle = {Single-{{Turnover Variable Chlorophyll Fluorescence}} as a {{Tool}} for {{Assessing Phytoplankton Photosynthesis}} and {{Primary Productivity}}},
  author = {Schuback, Nina and Tortell, Philippe D. and {Berman-Frank}, Ilana and Campbell, Douglas A. and Ciotti, Aurea and Courtecuisse, Emilie and Erickson, Zachary K. and Fujiki, Tetsuichi and Halsey, Kimberly and Hickman, Anna E. and Huot, Yannick and Gorbunov, Maxime Y. and Hughes, David J. and Kolber, Zbigniew S. and Moore, C. Mark and Oxborough, Kevin and Pr{\'a}{\v s}il, Ond{\v r}ej and Robinson, Charlotte M. and {Ryan-Keogh}, Thomas J. and Silsbe, Greg and Simis, Stefan and Suggett, David J. and Thomalla, Sandy and Varkey, Deepa R.},
  year = {2021},
  journal = {Frontiers in Marine Science},
  volume = {8},
  issn = {2296-7745},
  urldate = {2023-01-17},
  abstract = {Phytoplankton photosynthetic physiology can be investigated through single-turnover variable chlorophyll fluorescence (ST-ChlF) approaches, which carry unique potential to autonomously collect data at high spatial and temporal resolution. Over the past decades, significant progress has been made in the development and application of ST-ChlF methods in aquatic ecosystems, and in the interpretation of the resulting observations. At the same time, however, an increasing number of sensor types, sampling protocols, and data processing algorithms have created confusion and uncertainty among potential users, with a growing divergence of practice among different research groups. In this review, we assist the existing and upcoming user community by providing an overview of current approaches and consensus recommendations for the use of ST-ChlF measurements to examine in-situ phytoplankton productivity and photo-physiology. We argue that a consistency of practice and adherence to basic operational and quality control standards is critical to ensuring data inter-comparability. Large datasets of inter-comparable and globally coherent ST-ChlF observations hold the potential to reveal large-scale patterns and trends in phytoplankton photo-physiology, photosynthetic rates and bottom-up controls on primary productivity. As such, they hold great potential to provide invaluable physiological observations on the scales relevant for the development and validation of ecosystem models and remote sensing algorithms.},
  file = {/Users/tash/Zotero/storage/6LJWJ6KU/Schuback et al. - 2021 - Single-Turnover Variable Chlorophyll Fluorescence .pdf}
}

