  doi = {10.1038/316253a0},
  urldate = {2023-02-21},
  abstract = {The discovery of widely distributed populations of chroococcoid marine cyanobacteria (Synechococcus spp.) deep in the water column has focused attention on the autotrophic potential of small ({$<$}10 \textmu m) size classes of marine phytoplankton1\textendash 5. These organisms contain light-harvesting pigments which specifically absorb in the blue and green regions of the spectrum6\textendash 11. Despite the fact that sea water acts as a monochromatic filter, allowing only the blue and blue-green wavelengths to penetrate12, essentially all estimates of the photosynthetic rate of ultraphytoplankton at depth have ignored the changing spectral composition of the natural light field and simulated only the in situ light intensity4,5,13. Here I show that the spectral composition of available light must be considered in estimates of water column productivity and that the depth of the euphotic zone varies between different ultraphytoplankton strains depending on the composition and organization of the photosynthetic apparatus.},
  copyright = {1985 Nature Publishing Group},
  langid = {english},
  keywords = {Humanities and Social Sciences,multidisciplinary,Science}
}

