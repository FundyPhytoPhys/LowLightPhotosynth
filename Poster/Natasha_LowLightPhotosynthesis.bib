@article{geiderGrowthPhotosynthesisMaintenance1986,
  title = {Growth, {{Photosynthesis}} and {{Maintenance Metabolic Cost}} in the {{Diatom Phaeodactylum Tricornutum}} at {{Very Low Light Levels}} 1},
