@article{lyuModelingLightinducedElectric2017,
  title = {Modeling the Light-Induced Electric Potential Difference ({{$\Delta\Psi$}}), the {{pH}} Difference ({{$\Delta$pH}}) and the Proton Motive Force across the Thylakoid Membrane in {{C3}} Leaves},
  author = {Lyu, Hui and Laz{\'a}r, Du{\v s}an},
  year = {2017},
